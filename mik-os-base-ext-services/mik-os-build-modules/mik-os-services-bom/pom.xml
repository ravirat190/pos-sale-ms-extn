	<!-- 
	
		Start Distribution Management Settings aka Artifactory Settings

		snapshotRepository : Snapshot Repository of Artifactory

		repository		   : Release Repository of Artifactory

		Invocation		   : mvn clean deploy

		Usage			   : Will upload libraries to the respective artifactory based on version type is SNAPSHOT or RELEASE
						     refer this property above : cos.cr.version.type

		Note			   : Authentication entry to me made in conf/settings.xml of $MAVEN_HOME} like

							
							<server>
									<id>os-snapshots</id>
									<username>__username__</username>
									<password>__passwd__</password>
							</server>
							<server>
									<id>os-releases</id>
									<username>__username__</username>
									<password>__passwdz__</password>
							</server>
						</servers>
		TODO
	-->
	<distributionManagement>
		<snapshotRepository>
			<id>GAR Repo</id>
			<url>https://us-central1-maven.pkg.dev/prj-c-pos-infra-cicd-lwbf/pos-c-maven-standard</url>
		</snapshotRepository>
		<repository>
			<id>GAR Repo</id>
			<url>https://us-central1-maven.pkg.dev/prj-c-pos-infra-cicd-lwbf/pos-c-maven-standard</url>
		</repository>
	</distributionManagement>

	<!-- End Distribution Management Settings -->


