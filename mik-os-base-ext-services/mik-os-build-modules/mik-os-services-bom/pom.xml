		<hamcrest-core.version>1.3</hamcrest-core.version>
		<spring-test.version>4.1.4.RELEASE</spring-test.version>

		<javax.inject>1</javax.inject>
		<jsch.version>0.1.55</jsch.version>


		<jsch.version>0.1.55</jsch.version>

